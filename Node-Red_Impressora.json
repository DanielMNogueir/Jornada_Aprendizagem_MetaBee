    {
        "id": "fn_filtrar_mudanca",
        "type": "function",
        "z": "tab_monitor_3d_001",
        "name": "Filtrar mudança (só enviar se diferente)",
        "func": "// msg.payload é o status atual\nlet novoStatus = String(msg.payload || \"\");\nlet ultimoStatus = global.get(\"ultimoStatus\") || \"\";\n\nif (novoStatus !== ultimoStatus) {\n    global.set(\"ultimoStatus\", novoStatus);\n    // encaminha para enviar ao Discord\n    return msg;\n} else {\n    // ignora\n    return null;\n}\n",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 660,
        "y": 280,
        "wires": [
            [
                "fn_prepare_discord"
            ]
        ]
    },
    {
        "id": "fn_prepare_discord",
        "type": "function",
        "z": "tab_monitor_3d_001",
        "name": "Preparar payload Discord",
        "func": "// prepara o JSON que o webhook do Discord aceita\nlet status = String(msg.payload || \"\");\nlet distancia = global.get(\"distancia\") || null;\nlet data = {\n    username: \"Monitor 3D Printer\",\n    embeds: [\n        {\n            title: \"Status da Impressora 3D\",\n            description: `**${status}**\\nDistância: ${distancia !== null ? distancia+\" cm\" : \"N/A\"}`,\n            color: status.includes(\"funcionando\") ? 3066993 : 15158332,\n            timestamp: new Date().toISOString()\n        }\n    ]\n};\n\nmsg.payload = data;\nmsg.headers = { \"Content-Type\": \"application/json\" };\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
