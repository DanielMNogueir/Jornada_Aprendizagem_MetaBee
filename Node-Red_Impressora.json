    {
        "id": "fn_processa",
        "type": "function",
        "z": "tab_monitor_3d_001",
        "name": "Processar Dados (sensor → estado)",
        "func": "// msg.payload contém a distância (em cm)\nlet distancia = Number(msg.payload);\nif (isNaN(distancia)) return null;\n\n// Ajuste o limite conforme sua impressora\nlet limiteParado = 10; // < 10cm => parada (ajuste conforme necessário)\nlet status = (distancia < limiteParado) ? \"Impressora parada\" : \"Impressora funcionando\";\n\n// Salva globalmente para endpoint HTTP\nglobal.set(\"distancia\", distancia);\nglobal.set(\"status\", status);\nglobal.set(\"ultimaAtualizacao\", new Date().toISOString());\n\n// Saída 1 -> Gauge (valor numérico)\nlet out1 = { payload: distancia };\n// Saída 2 -> ui_text (texto de status)\nlet out2 = { payload: status };\n// Saída 3 -> WebSocket broadcast (obj com dados)\nlet out3 = { payload: JSON.stringify({ distancia: distancia, status: status, timestamp: new Date().toISOString() }) };\n// Saída 4 -> Mensagem para o filtro de mudanças (Status simples)\nlet out4 = { payload: status };\n\nreturn [out1, out2, out3, out4];",
        "outputs": 4,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 420,
        "y": 140,
        "wires": [
            [
                "ui_gauge_dist"
            ],
            [
                "ui_text_status"
            ],
            [
                "ws_out_impressora"
            ],
            [
                "fn_filtrar_mudanca"
            ]
        ]
    },
